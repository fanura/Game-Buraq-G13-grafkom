; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="Blaster Faster"
run/main_scene="res://stages/stage_menu.tscn"
config/icon="res://icon.png"

[autoload]

utils="*res://scripts/utils.gd"
audio_player="*res://scenes/audio_player.tscn"
saver="*res://scripts/saver.gd"

[display]

window/size/width=180
window/size/height=320
window/size/test_width=360
window/size/test_height=640
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

2d/snapping/use_gpu_pixel_snap=true
environment/default_clear_color=Color( 0.356863, 0.0313726, 0.00392157, 1 )
environment/default_environment="res://default_env.tres"
quality/2d/use_pixel_snap=true
